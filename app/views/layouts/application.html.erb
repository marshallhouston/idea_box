<!DOCTYPE html>
<html>
  <head>
    <title>IdeaBox</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>
  <header>
    <% if current_user %>
      <%= link_to "Log Out", logout_path, method: :delete %>
    <% end %>
  </header><br>
  <body>
    <%= yield %>
  </body>
</html>
